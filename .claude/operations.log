nsition-duration:0.01ms !important}}@media (prefers-contrast:high){:root{--color-primary:#000000;--color-accent:#0066CC;--color-text:#000000}}\n    </style>\n    \n    <!-- Fonts -->\n    <link href=\"https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600&family=Space+Grotesk:wght@500;700&display=swap\" rel=\"stylesheet\">\n    \n    <!-- PWA Meta -->\n    <meta name=\"application-name\" content=\"onmedic\">\n    <meta name=\"apple-mobile-web-app-title\" content=\"onmedic\">\n    <meta name=\"apple-mobile-web-app-capable\" content=\"yes\">\n    <meta name=\"apple-mobile-web-app-status-bar-style\" content=\"default\">\n    <meta name=\"mobile-web-app-capable\" content=\"yes\">\n    <meta name=\"msapplication-TileColor\" content=\"#0A2342\">\n    <meta name=\"msapplication-navbutton-color\" content=\"#0A2342\">\n    \n    <!-- Geo tags -->\n    <meta name=\"geo.region\" content=\"US\">\n    <meta name=\"geo.placename\" content=\"United States\">\n    <meta name=\"ICBM\" content=\"37.0902, -95.7129\">\n    \n    <!-- Additional SEO -->\n    <meta name=\"robots\" content=\"index, follow, max-snippet:-1, max-image-preview:large, max-video-preview:-1\">\n    <meta name=\"googlebot\" content=\"index, follow\">\n    <meta name=\"author\" content=\"onmedic\">\n    <meta name=\"publisher\" content=\"onmedic\">\n    <link rel=\"author\" href=\"https://onmedic.com\">\n    \n    <!-- DNS Prefetch -->\n    <link rel=\"dns-prefetch\" href=\"//fonts.googleapis.com\">\n    <link rel=\"dns-prefetch\" href=\"//fonts.gstatic.com\">\n    <link rel=\"dns-prefetch\" href=\"//formspree.io\">\n    \n    <!-- Critical CSS Inline for Above-the-Fold Performance -->\n    <style>\n        /* CSS Variables and Reset */\n        *,*::before,*::after{box-sizing:border-box;margin:0;padding:0}\n        :root{--color-primary:#0A2342;--color-accent:#00D4FF;--color-secondary:#6C7293;--color-background:#F8F9FD;--color-text:#1A1A2E;--color-white:#ffffff;--font-heading:'Space Grotesk',-apple-system,BlinkMacSystemFont,sans-serif;--font-body:'Inter',-apple-system,BlinkMacSystemFont,sans-serif;--gradient-hero:linear-gradient(135deg,var(--color-primary) 0%,var(--color-accent) 100%);--shadow-card:0 10px 40px rgba(10,35,66,0.1);--border-radius:12px}\n        \n        /* Body and HTML */\n        html{scroll-behavior:smooth;scroll-padding-top:120px}\n        body{font-family:var(--font-body);font-size:16px;line-height:1.6;color:var(--color-text);background-color:var(--color-background);-webkit-font-smoothing:antialiased;-moz-osx-font-smoothing:grayscale}\n        \n        /* Fixed Header */\n        .header{position:fixed;top:0;left:0;right:0;background:rgba(255,255,255,0.95);border-bottom:1px solid rgba(10,35,66,0.08);box-shadow:0 2px 20px rgba(10,35,66,0.06);z-index:1000;padding:1.2rem 0;transition:all 0.3s ease}\n        .nav-container{max-width:1200px;margin:0 auto;padding:0 2rem;display:flex;justify-content:space-between;align-items:center}\n        .logo{display:flex;align-items:center;text-decoration:none}\n        .logo img{height:65px;width:auto;transition:opacity 0.3s ease}\n        .main-nav{display:flex;align-items:center}\n        .nav-menu{display:flex;align-items:center;gap:0;list-style:none;margin:0;padding:0}\n        .nav-link{color:var(--color-secondary);text-decoration:none;font-weight:400;font-size:0.95rem;transition:all 0.2s ease;padding:0.8rem 1.2rem;border-radius:6px;margin:0 0.2rem}\n        .nav-link:hover{color:var(--color-primary);background:rgba(10,35,66,0.04)}\n        .nav-cta{background:var(--color-primary);color:var(--color-white)!important;padding:0.8rem 1.8rem;border-radius:6px;margin-left:0.8rem;font-weight:500}\n        .nav-cta:hover{background:var(--color-accent);color:var(--color-white)!important}\n        \n        /* Hero Section */\n        .hero{min-height:100vh;background:var(--gradient-hero);display:flex;align-items:center;justify-content:center;position:relative;overflow:hidden;padding-top:120px}\n        .hero-container{max-width:1200px;margin:0 auto;padding:0 2rem;text-align:center;z-index:2}\n        .hero h1{font-family:var(--font-heading);font-size:clamp(2.5rem,5vw,4rem);font-weight:700;color:var(--color-white);margin-bottom:1.5rem;line-height:1.2}\n        .hero-subtitle{font-size:clamp(1.1rem,2.5vw,1.5rem);color:rgba(255,255,255,0.9);margin-bottom:2.5rem;max-width:600px;margin-left:auto;margin-right:auto}\n        .btn-primary{display:inline-block;background:var(--color-white);color:var(--color-primary);padding:1rem 2rem;border-radius:var(--border-radius);text-decoration:none;font-weight:600;font-size:1.1rem;transition:all 0.3s ease;border:none;cursor:pointer}\n        .btn-primary:hover{transform:translateY(-2px);box-shadow:0 8px 25px rgba(0,212,255,0.3)}\n        \n        /* Animation canvas */\n        #hero-animation{width:100%;height:100%;position:absolute;top:0;left:0;z-index:1;pointer-events:none}\n        \n        /* Critical Responsive */\n        @media (max-width:768px){\n            .nav-container{padding:0 1rem}\n            .hero-container{padding:0 1rem}\n            .nav-link{padding:0.6rem 0.8rem;font-size:0.9rem}\n            .nav-cta{padding:0.6rem 1.2rem;font-size:0.9rem;margin-left:0.4rem}\n            .btn-primary{padding:0.8rem 1.5rem;font-size:1rem}\n        }\n        \n        /* Prefers Reduced Motion */\n        @media (prefers-reduced-motion:reduce){\n            html{scroll-behavior:auto}\n            *{animation-duration:0.01ms!important;animation-iteration-count:1!important;transition-duration:0.01ms!important}\n        }\n    </style>\n    \n    <!-- Non-critical CSS loaded asynchronously -->\n    <link rel=\"preload\" href=\"/src/styles/main.min.css\" as=\"style\" onload=\"this.onload=null;this.rel='stylesheet'\">\n    <noscript><link rel=\"stylesheet\" href=\"/src/styles/main.min.css\"></noscript>\n    \n    <!-- Schema.org Structured Data -->\n    <script type=\"application/ld+json\">\n    {\n        \"@context\": \"https://schema.org\",\n        \"@type\": \"TechnologyCompany\",\n        \"name\": \"onmedic\",\n        \"url\": \"https://onmedic.com\",\n        \"description\": \"Technology company specialized in advanced solutions with Artificial Intelligence for the pharmaceutical industry, hospitals and healthcare institutions.\",\n        \"foundingDate\": \"2020\",\n        \"industry\": \"Healthcare Technology\",\n        \"sameAs\": [],\n        \"areaServed\": \"Worldwide\",\n        \"serviceType\": [\n            \"Artificial Intelligence Solutions\",\n            \"Healthcare Technology Development\",\n            \"Pharmaceutical Technology\",\n            \"Hospital Information Systems\",\n            \"Digital Health Transformation\"\n        ],\n        \"contactPoint\": {\n            \"@type\": \"ContactPoint\",\n            \"contactType\": \"Business Inquiries\",\n            \"email\": \"contact@onmedic.com\"\n        }\n    }\n    </script>\n</head>\n<body>\n    <!-- Header -->\n    <header class=\"header\" id=\"header\">\n        <div class=\"nav-container\">\n            <a href=\"#\" class=\"logo\">\n                <picture>\n                    <source srcset=\"/src/assets/images/logo-onmedic.webp\" type=\"image/webp\">\n                    <img src=\"/src/assets/images/logo-onmedic.jpg\" alt=\"onmedic - Building eHealth\" width=\"200\" height=\"65\">\n                </picture>\n            </a>\n            <nav class=\"main-nav\">\n                <ul class=\"nav-menu\">\n                    <li><a href=\"#about\" class=\"nav-link\">Innovation</a></li>\n                    <li><a href=\"#expertise\" class=\"nav-link\">Expertise</a></li>\n                    <li><a href=\"contact.html\" class=\"nav-link nav-cta\">Contact</a></li>\n                </ul>\n            </nav>\n        </div>\n    </header>\n\n    <!-- Hero Section -->\n    <section class=\"hero\" id=\"hero\">\n        <div class=\"hero-container\">\n            <h1>Healthcare, Reimagined</h1>\n            <p class=\"hero-subtitle\">\n                Look, technology is changing the world. And we make this \n                revolution reach hospitals and pharmaceuticals everywhere\n            </p>\n            <a href=\"#about\" class=\"btn-primary\" role=\"button\" aria-label=\"Discover more about onmedic - Go to innovation section\">Discover the Future</a>\n        </div>\n        \n        <!-- Background animation (will be loaded with JS) -->\n        <canvas id=\"hero-animation\" style=\"position: absolute; top: 0; left: 0; width: 100%; height: 100%; z-index: 1;\"></canvas>\n    </section>\n\n    <!-- Who We Are -->\n    <section class=\"about\" id=\"about\">\n        <div class=\"container\">\n            <div class=\"section-header\">\n                <h2>Innovation? Yes. But With Purpose</h2>\n                <p class=\"section-subtitle\">\n                    We're not just another tech company... At onmedic we understand what \n                    doctors and nurses really need. Artificial Intelligence? It's fantastic! \n                    But what really matters is how we use it to help people.\n                </p>\n            </div>\n            \n            <div class=\"features-grid\">\n                <div class=\"feature-card\" data-aos=\"fade-up\" data-aos-delay=\"100\">\n                    <div class=\"feature-icon\">\n                        <svg width=\"48\" height=\"48\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                            <path d=\"M9 12l2 2 4-4\"/>\n                            <path d=\"M21 12c-1 0-3-1-3-3s2-3 3-3 3 1 3 3-2 3-3 3\"/>\n                            <path d=\"M3 12c1 0 3-1 3-3s-2-3-3-3-3 1-3 3 2 3 3 3\"/>\n                            <path d=\"M3 12h6l4-4 4 4h6\"/>\n                        </svg>\n                    </div>\n                    <h3>AI (The Good Kind)</h3>\n                    <p>Our algorithms? They work. They help with complicated diagnoses and make everything faster at the hospital.</p>\n                </div>\n                \n                <div class=\"feature-card\" data-aos=\"fade-up\" data-aos-delay=\"200\">\n                    <div class=\"feature-icon\">\n                        <svg width=\"48\" height=\"48\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                            <path d=\"M18 10h-1.26A8 8 0 1 0 9 20h9a5 5 0 0 0 0-10z\"/>\n                        </svg>\n                    </div>\n                    <h3>Cloud (But Secure, Right?)</h3>\n                    <p>Patient data is sacred. That's why we use the most secure cloud that exists. And yes, it scales when needed!</p>\n                </div>\n                \n                <div class=\"feature-card\" data-aos=\"fade-up\" data-aos-delay=\"300\">\n                    <div class=\"feature-icon\">\n                        <svg width=\"48\" height=\"48\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                            <rect x=\"3\" y=\"3\" width=\"18\" height=\"18\" rx=\"2\"/>\n                            <path d=\"M3 12h18\"/>\n                            <path d=\"M12 3v18\"/>\n                        </svg>\n                    </div>\n                    <h3>Data That Speaks</h3>\n                    <p>You know what? Data never lies. We listen to it, understand it... and explain what it really means.</p>\n                </div>\n                \n                <div class=\"feature-card\" data-aos=\"fade-up\" data-aos-delay=\"400\">\n                    <div class=\"feature-icon\">\n                        <svg width=\"48\" height=\"48\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\">\n                            <path d=\"M12 22s8-4 8-10V5l-8-3-8 3v7c0 6 8 10 8 10z\"/>\n                            <path d=\"M9 12l2 2 4-4\"/>\n                        </svg>\n                    </div>\n                    <h3>Rock Solid Security</h3>\n                    <p>Nobody gets in where they shouldn't. Period. We comply with all international standards (and some we invented ourselves).</p>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <!-- Areas of Expertise -->\n    <section class=\"expertise\" id=\"expertise\">\n        <div class=\"container\">\n            <div class=\"section-header\">\n                <h2>What We're Really Good At</h2>\n                <p class=\"section-subtitle\">\n                    We have experience, yes. But most importantly... we know how to listen to what each client needs\n                </p>\n            </div>\n            \n            <div class=\"expertise-grid\">\n                <div class=\"expertise-card\" data-aos=\"fade-up\" data-aos-delay=\"100\">\n                    <div class=\"expertise-icon\">\n                        <svg width=\"56\" height=\"56\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"1.5\">\n                            <path d=\"M3 3v5h5\"/>\n                            <path d=\"M21 21v-5h-5\"/>\n                            <path d=\"M7 17l4-4 4 4\"/>\n                            <path d=\"M17 7l-4 4-4-4\"/>\n                        </svg>\n                    </div>\n                    <h3>Understanding Medical Data</h3>\n                    <p>There's tons of data in hospitals. We organize it, analyze it and - best of all - make it understandable for everyone!</p>\n                </div>\n\n                <div class=\"expertise-card\" data-aos=\"fade-up\" data-aos-delay=\"200\">\n                    <div class=\"expertise-icon\">\n                        <svg width=\"56\" height=\"56\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"1.5\">\n                            <path d=\"M12 22s8-4 8-10V5l-8-3-8 3v7c0 6 8 10 8 10z\"/>\n                            <path d=\"M9 12l2 2 4-4\"/>\n                        </svg>\n                    </div>\n                    <h3>Real Security</h3>\n                    <p>GDPR, HIPAA... yes, we comply with all that. But also, we like to sleep soundly knowing the data is protected.</p>\n                </div>\n\n                <div class=\"expertise-card\" data-aos=\"fade-up\" data-aos-delay=\"300\">\n                    <div class=\"expertise-icon\">\n                        <svg width=\"56\" height=\"56\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"1.5\">\n                            <path d=\"M13 2L3 14h9l-1 8 10-12h-9l1-8z\"/>\n                        </svg>\n                    </div>\n                    <h3>From Paper to Digital (No Fear)</h3>\n                    <p>Still with folders and papers? No worries! We accompany you step by step to the digital world. It's not as hard as it seems, really.</p>\n                </div>\n\n                <div class=\"expertise-card\" data-aos=\"fade-up\" data-aos-delay=\"400\">\n                    <div class=\"expertise-icon\">\n                        <svg width=\"56\" height=\"56\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"1.5\">\n                            <path d=\"M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z\"/>\n                            <polyline points=\"9,22 9,12 15,12 15,22\"/>\n                        </svg>\n                    </div>\n                    <h3>Hospitals That Work Better</h3>\n                    <p>From the doctor's schedule to the medicine warehouse... We connect everything. And suddenly, magic: everything flows!</p>\n                </div>\n\n                <div class=\"expertise-card\" data-aos=\"fade-up\" data-aos-delay=\"500\">\n                    <div class=\"expertise-icon\">\n                        <svg width=\"56\" height=\"56\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"1.5\">\n                            <path d=\"M21 15a2 2 0 0 1-2 2H7l-4 4V5a2 2 0 0 1 2-2h14a2 2 0 0 1 2 2z\"/>\n                            <path d=\"M13 9h2l2 2-2 2h-2\"/>\n                            <path d=\"M11 9H9L7 11l2 2h2\"/>\n                        </svg>\n                    </div>\n                    <h3>Bluesky & YouTube? We Master Them!</h3>\n                    <p>These platforms have no secrets from us. We know how they work inside and out. And yes, we can help you profit from them.</p>\n                </div>\n\n                <div class=\"expertise-card\" data-aos=\"fade-up\" data-aos-delay=\"600\">\n                    <div class=\"expertise-icon\">\n                        <svg width=\"56\" height=\"56\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"1.5\">\n                            <rect x=\"2\" y=\"3\" width=\"20\" height=\"14\" rx=\"2\" ry=\"2\"/>\n                            <line x1=\"8\" y1=\"21\" x2=\"16\" y2=\"21\"/>\n                            <line x1=\"12\" y1=\"17\" x2=\"12\" y2=\"21\"/>\n                            <circle cx=\"7\" cy=\"8\" r=\"1\"/>\n                            <circle cx=\"17\" cy=\"8\" r=\"1\"/>\n                        </svg>\n                    </div>\n                    <h3>Apps For Everything and Everyone</h3>\n                    <p>iPhone? Android? Web? 2015 tablet? Don't worry, we make it work everywhere. It's our specialty!</p>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <!-- Footer -->\n    <footer class=\"footer\" id=\"footer\">\n        <div class=\"container\">            \n            <div class=\"footer-bottom\">\n                <p>&copy; <span id=\"current-year\"></span> <a href=\"/\">onmedic</a>. Building eHealth. All rights reserved. | <a href=\"terms.html\">Terms of Use</a> | <a href=\"privacy.html\">Privacy</a></p>\n            </div>\n        </div>\n    </footer>\n\n    <!-- Non-blocking Scripts -->\n    <script defer src=\"/src/scripts/main.min.js\"></script>\n    <script defer src=\"/src/scripts/animations.min.js\"></script>\n    \n    <!-- Cookie Banner -->\n    <div id=\"cookie-consent-banner\" style=\"display: none; position: fixed; bottom: 0; left: 0; right: 0; background: rgba(10, 35, 66, 0.95); color: white; padding: 1rem; z-index: 10000; backdrop-filter: blur(10px); border-top: 1px solid rgba(255,255,255,0.1);\">\n        <div style=\"max-width: 1200px; margin: 0 auto; display: flex; align-items: center; justify-content: space-between; flex-wrap: wrap; gap: 1rem;\">\n            <div style=\"flex: 1; min-width: 200px;\">\n                <p style=\"margin: 0; font-size: 0.9rem; line-height: 1.4;\">\n                    \ud83c\udf6a We use cookies to improve your experience. \n                    <a href=\"cookies.html\" style=\"color: #00D4FF; text-decoration: underline;\">More information</a>\n                </p>\n            </div>\n            <div style=\"display: flex; gap: 0.5rem; flex-wrap: wrap;\">\n                <button onclick=\"acceptAllCookies()\" style=\"background: #00D4FF; color: white; border: none; padding: 0.5rem 1rem; border-radius: 6px; font-size: 0.85rem; cursor: pointer; font-weight: 500;\">\n                    Accept\n                </button>\n                <button onclick=\"rejectOptionalCookies()\" style=\"background: transparent; color: white; border: 1px solid rgba(255,255,255,0.3); padding: 0.5rem 1rem; border-radius: 6px; font-size: 0.85rem; cursor: pointer;\">\n                    Essential only\n                </button>\n            </div>\n        </div>\n    </div>\n\n    <!-- Dynamic year script -->\n    <script>\n        document.getElementById('current-year').textContent = new Date().getFullYear();\n        \n        // Cookie management\n        function getCookieConsent() {\n            return localStorage.getItem('cookieConsent');\n        }\n        \n        function setCookieConsent(value) {\n            localStorage.setItem('cookieConsent', value);\n            localStorage.setItem('cookieConsentDate', new Date().toISOString());\n        }\n        \n        function showCookieBanner() {\n            const consent = getCookieConsent();\n            if (!consent) {\n                document.getElementById('cookie-consent-banner').style.display = 'block';\n            }\n        }\n        \n        function hideCookieBanner() {\n            document.getElementById('cookie-consent-banner').style.display = 'none';\n        }\n        \n        function acceptAllCookies() {\n            setCookieConsent('all');\n            hideCookieBanner();\n            // Here you could activate optional cookies\n            console.log('Cookies accepted: all');\n        }\n        \n        function rejectOptionalCookies() {\n            setCookieConsent('essential');\n            hideCookieBanner();\n            // Keep only necessary cookies\n            console.log('Cookies accepted: essential only');\n        }\n        \n        // Show banner if needed\n        document.addEventListener('DOMContentLoaded', function() {\n            setTimeout(showCookieBanner, 1500); // Wait 1.5 seconds before showing\n        });\n    </script>\n</body>\n</html>"}
