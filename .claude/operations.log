[2025-08-22 17:47:04] Write: {"file_path": "/Users/albertarno/Documents/_codi/onmedic.github.io/404.html", "content": "<!DOCTYPE html>\n<html lang=\"en\" dir=\"ltr\">\n<head>\n    <!-- Basic Meta -->\n    <meta charset=\"utf-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    \n    <!-- SEO Core -->\n    <title>404 - Page Not Found | onmedic</title>\n    <meta name=\"description\" content=\"The page you're looking for doesn't exist. Return to onmedic homepage.\">\n    <meta name=\"robots\" content=\"noindex, nofollow\">\n    \n    <!-- Canonical -->\n    <link rel=\"canonical\" href=\"https://onmedic.com/404.html\">\n    \n    <!-- CSS -->\n    <link rel=\"preload\" href=\"/src/styles/main.min.css\" as=\"style\" onload=\"this.onload=null;this.rel='stylesheet'\">\n    <noscript><link rel=\"stylesheet\" href=\"/src/styles/main.min.css\"></noscript>\n    \n    <!-- Fonts -->\n    <link href=\"https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600&family=Space+Grotesk:wght@500;700&display=swap\" rel=\"stylesheet\">\n    \n    <style>\n        :root{--color-primary:#0A2342;--color-accent:#00D4FF;--color-secondary:#6C7293;--color-background:#F8F9FD;--color-text:#1A1A2E;--color-white:#ffffff;--font-heading:'Space Grotesk',-apple-system,BlinkMacSystemFont,sans-serif;--font-body:'Inter',-apple-system,BlinkMacSystemFont,sans-serif;--border-radius:12px}\n        body{font-family:var(--font-body);color:var(--color-text);background:var(--color-background);margin:0;padding:0;line-height:1.6}\n        .container{max-width:1200px;margin:0 auto;padding:0 2rem}\n        .header{position:fixed;top:0;left:0;right:0;background:rgba(255,255,255,0.95);border-bottom:1px solid rgba(10,35,66,0.08);box-shadow:0 2px 20px rgba(10,35,66,0.06);z-index:1000;padding:1.2rem 0;transition:all 0.3s ease}\n        .nav-container{max-width:1200px;margin:0 auto;padding:0 2rem;display:flex;justify-content:space-between;align-items:center}\n        .logo{display:flex;align-items:center;text-decoration:none}\n        .logo img{height:65px;width:auto}\n        .main-nav{display:flex;align-items:center}\n        .nav-menu{display:flex;align-items:center;gap:0;list-style:none;margin:0;padding:0}\n        .nav-link{color:var(--color-secondary);text-decoration:none;font-weight:400;font-size:0.95rem;transition:all 0.2s ease;padding:0.8rem 1.2rem;border-radius:6px;margin:0 0.2rem}\n        .nav-link:hover{color:var(--color-primary);background:rgba(10,35,66,0.04)}\n        .nav-cta{background:var(--color-primary);color:var(--color-white)!important;padding:0.8rem 1.8rem;border-radius:6px;margin-left:0.8rem;font-weight:500}\n        .error-content{padding:8rem 0 4rem 0;text-align:center}\n        .error-container{max-width:600px;margin:0 auto;padding:0 2rem}\n        .error-code{font-family:var(--font-heading);font-size:8rem;font-weight:700;color:var(--color-accent);margin:0;line-height:1}\n        .error-title{font-family:var(--font-heading);font-size:2.5rem;color:var(--color-primary);margin:1rem 0}\n        .error-message{font-size:1.1rem;color:var(--color-text);margin-bottom:2rem}\n        .error-actions{margin:2rem 0}\n        .btn-primary{background:var(--color-primary);color:var(--color-white);padding:1rem 2rem;border-radius:8px;text-decoration:none;font-weight:500;display:inline-block;margin:0 0.5rem;transition:all 0.2s ease}\n        .btn-primary:hover{background:var(--color-accent);color:var(--color-primary)}\n        .btn-secondary{background:transparent;color:var(--color-primary);padding:1rem 2rem;border:2px solid var(--color-primary);border-radius:8px;text-decoration:none;font-weight:500;display:inline-block;margin:0 0.5rem;transition:all 0.2s ease}\n        .btn-secondary:hover{background:var(--color-primary);color:var(--color-white)}\n        .footer{background:var(--color-primary);color:var(--color-white);padding:3rem 0 1rem;margin-top:4rem}\n        .footer-bottom{text-align:center;padding-top:2rem;border-top:1px solid rgba(255,255,255,0.1);color:rgba(255,255,255,0.7)}\n        .footer-bottom a{color:var(--color-accent);text-decoration:none}\n        .footer-bottom a:hover{text-decoration:underline}\n        @media (max-width:768px){.error-code{font-size:6rem}.error-title{font-size:2rem}.btn-primary,.btn-secondary{display:block;margin:0.5rem 0}}\n    </style>\n</head>\n<body>\n    <!-- Header -->\n    <header class=\"header\" id=\"header\">\n        <div class=\"nav-container\">\n            <a href=\"/\" class=\"logo\">\n                <picture>\n                    <source srcset=\"/src/assets/images/logo-onmedic.webp\" type=\"image/webp\">\n                    <img src=\"/src/assets/images/logo-onmedic.jpg\" alt=\"onmedic - Building eHealth\" width=\"200\" height=\"65\">\n                </picture>\n            </a>\n            <nav class=\"main-nav\">\n                <ul class=\"nav-menu\">\n                    <li><a href=\"#\" onclick=\"goToLanguageHome()\" class=\"nav-link\">Home</a></li>\n                    <li><a href=\"#\" onclick=\"goToContact()\" class=\"nav-link nav-cta\">Contact</a></li>\n                </ul>\n            </nav>\n        </div>\n    </header>\n\n    <!-- Error Content -->\n    <main class=\"error-content\">\n        <div class=\"error-container\">\n            <div class=\"error-code\">404</div>\n            <h1 class=\"error-title\">Page Not Found</h1>\n            <p class=\"error-message\">The page you're looking for doesn't exist or may have been moved.</p>\n            \n            <div class=\"error-actions\">\n                <a href=\"#\" onclick=\"goToLanguageHome()\" class=\"btn-primary\">Go Home</a>\n                <a href=\"#\" onclick=\"goBack()\" class=\"btn-secondary\">Go Back</a>\n            </div>\n        </div>\n    </main>\n\n    <!-- Footer -->\n    <footer class=\"footer\">\n        <div class=\"container\">            \n            <div class=\"footer-bottom\">\n                <p>&copy; <span id=\"current-year\"></span> <a href=\"/\">onmedic</a>. Building eHealth. All rights reserved.</p>\n            </div>\n        </div>\n    </footer>\n\n    <!-- Scripts -->\n    <script>\n        // Dynamic year\n        document.getElementById('current-year').textContent = new Date().getFullYear();\n        \n        // Language detection and navigation\n        function detectLanguage() {\n            // Check for language in localStorage\n            const savedLang = localStorage.getItem('preferred-language');\n            if (savedLang && ['ca', 'es', 'en'].includes(savedLang)) {\n                return savedLang;\n            }\n            \n            // Check browser languages\n            const browserLangs = navigator.languages || [navigator.language];\n            \n            for (let lang of browserLangs) {\n                const langCode = lang.split('-')[0].toLowerCase();\n                if (langCode === 'ca') return 'ca';\n                if (langCode === 'es') return 'es';\n                if (langCode === 'en') return 'en';\n            }\n            \n            // Default fallback to English\n            return 'en';\n        }\n        \n        function goToLanguageHome() {\n            const lang = detectLanguage();\n            window.location.href = `/${lang}/`;\n        }\n        \n        function goToContact() {\n            const lang = detectLanguage();\n            const contactPages = {\n                'ca': '/ca/contacte.html',\n                'es': '/es/contacto.html',\n                'en': '/en/contact.html'\n            };\n            window.location.href = contactPages[lang];\n        }\n        \n        function goBack() {\n            if (window.history.length > 1) {\n                window.history.back();\n            } else {\n                goToLanguageHome();\n            }\n        }\n        \n        // Auto-redirect after 5 seconds if user doesn't interact\n        setTimeout(function() {\n            if (!document.hidden) {\n                goToLanguageHome();\n            }\n        }, 5000);\n    </script>\n</body>\n</html>"}
