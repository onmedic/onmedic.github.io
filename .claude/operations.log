[2025-08-22 12:18:30] mcp__desktop-commander__write_file: {"path": "/Users/albertarno/Documents/_codi/onmedic.github.io/src/styles/contact.css", "content": "/* Contact Page Styles */\n\n/* Contact Hero */\n.contact-hero {\n    min-height: 50vh;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    background: linear-gradient(135deg, var(--color-primary) 0%, var(--color-accent) 100%);\n    position: relative;\n    overflow: hidden;\n}\n\n.contact-hero .hero-container {\n    max-width: 800px;\n    text-align: center;\n    z-index: 2;\n}\n\n/* Contact Section */\n.contact-section {\n    background: var(--color-background);\n    padding: 6rem 0;\n}\n\n.contact-content {\n    display: grid;\n    grid-template-columns: 1fr 1fr;\n    gap: 4rem;\n    align-items: start;\n}\n\n/* Contact Info */\n.contact-info .section-header {\n    text-align: left;\n    margin-bottom: 3rem;\n}\n\n.contact-methods {\n    display: flex;\n    flex-direction: column;\n    gap: 2rem;\n}\n\n.contact-method {\n    display: flex;\n    align-items: flex-start;\n    gap: 1rem;\n    padding: 1.5rem;\n    background: var(--color-white);\n    border-radius: var(--border-radius);\n    box-shadow: 0 4px 15px rgba(10, 35, 66, 0.08);\n    border: 1px solid rgba(10, 35, 66, 0.05);\n    transition: all 0.3s ease;\n}\n\n.contact-method:hover {\n    transform: translateY(-2px);\n    box-shadow: 0 8px 25px rgba(10, 35, 66, 0.15);\n}\n\n.contact-icon {\n    color: var(--color-accent);\n    background: rgba(0, 212, 255, 0.1);\n    padding: 0.75rem;\n    border-radius: 8px;\n    flex-shrink: 0;\n}\n\n.contact-method h3 {\n    font-family: var(--font-heading);\n    font-size: 1.2rem;\n    font-weight: 600;\n    color: var(--color-primary);\n    margin-bottom: 0.5rem;\n}\n\n.contact-method p {\n    color: var(--color-secondary);\n    line-height: 1.5;\n    margin: 0;\n}\n\n/* Contact Form Container */\n.contact-form-container {\n    background: var(--color-white);\n    padding: 3rem;\n    border-radius: var(--border-radius);\n    box-shadow: 0 10px 40px rgba(10, 35, 66, 0.1);\n    border: 1px solid rgba(10, 35, 66, 0.05);\n}\n\n/* Form Styles */\n.contact-form .form-row {\n    display: grid;\n    grid-template-columns: 1fr 1fr;\n    gap: 1.5rem;\n    margin-bottom: 1.5rem;\n}\n\n.contact-form .form-group {\n    margin-bottom: 1.5rem;\n}\n\n.contact-form .form-group label {\n    display: block;\n    margin-bottom: 0.5rem;\n    font-weight: 500;\n    color: var(--color-primary);\n    font-size: 0.95rem;\n}\n\n.contact-form .form-group input,\n.contact-form .form-group textarea,\n.contact-form .form-group select {\n    width: 100%;\n    padding: 1rem;\n    border: 1px solid rgba(10, 35, 66, 0.2);\n    border-radius: 8px;\n    background: var(--color-white);\n    color: var(--color-text);\n    font-family: var(--font-body);\n    font-size: 1rem;\n    transition: all 0.3s ease;\n}\n\n.contact-form .form-group input::placeholder,\n.contact-form .form-group textarea::placeholder {\n    color: rgba(26, 26, 46, 0.5);\n}\n\n.contact-form .form-group input:focus,\n.contact-form .form-group textarea:focus,\n.contact-form .form-group select:focus {\n    outline: none;\n    border-color: var(--color-accent);\n    box-shadow: 0 0 0 3px rgba(0, 212, 255, 0.1);\n    background: rgba(0, 212, 255, 0.02);\n}\n\n.contact-form .form-group textarea {\n    resize: vertical;\n    min-height: 120px;\n}\n\n.contact-form .form-group select {\n    cursor: pointer;\n    background-image: url(\"data:image/svg+xml;charset=utf-8,%3Csvg xmlns='http://www.w3.org/2000/svg' fill='none' viewBox='0 0 24 24'%3E%3Cpath stroke='%236C7293' stroke-linecap='round' stroke-linejoin='round' stroke-width='1.5' d='M6 9l6 6 6-6'/%3E%3C/svg%3E\");\n    background-repeat: no-repeat;\n    background-position: right 1rem center;\n    background-size: 1rem;\n    -webkit-appearance: none;\n    -moz-appearance: none;\n    appearance: none;\n}\n\n/* Checkbox Group */\n.checkbox-group {\n    display: flex;\n    align-items: flex-start;\n    gap: 0.75rem;\n}\n\n.checkbox-label {\n    display: flex;\n    align-items: flex-start;\n    gap: 0.75rem;\n    cursor: pointer;\n    line-height: 1.5;\n    font-size: 0.95rem;\n}\n\n.checkbox-label input[type=\"checkbox\"] {\n    width: auto;\n    margin: 0;\n    opacity: 0;\n    position: absolute;\n}\n\n.checkbox-label .checkmark {\n    width: 20px;\n    height: 20px;\n    border: 2px solid rgba(10, 35, 66, 0.3);\n    border-radius: 4px;\n    background: var(--color-white);\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    transition: all 0.3s ease;\n    flex-shrink: 0;\n    margin-top: 2px;\n}\n\n.checkbox-label input[type=\"checkbox\"]:checked + .checkmark {\n    background: var(--color-accent);\n    border-color: var(--color-accent);\n}\n\n.checkbox-label input[type=\"checkbox\"]:checked + .checkmark::after {\n    content: \"\u2713\";\n    color: var(--color-white);\n    font-size: 0.8rem;\n    font-weight: 600;\n}\n\n.checkbox-label input[type=\"checkbox\"]:focus + .checkmark {\n    box-shadow: 0 0 0 3px rgba(0, 212, 255, 0.2);\n}\n\n.checkbox-label a {\n    color: var(--color-accent);\n    text-decoration: none;\n}\n\n.checkbox-label a:hover {\n    text-decoration: underline;\n}\n\n/* Button Styles */\n.contact-form .btn-primary {\n    background: linear-gradient(135deg, var(--color-primary), var(--color-accent));\n    color: var(--color-white);\n    border: none;\n    display: inline-flex;\n    align-items: center;\n    gap: 0.5rem;\n    justify-content: center;\n    width: 100%;\n    margin-top: 1rem;\n}\n\n.contact-form .btn-primary:hover {\n    background: linear-gradient(135deg, var(--color-accent), var(--color-primary));\n    transform: translateY(-2px);\n    box-shadow: 0 8px 25px rgba(0, 212, 255, 0.3);\n}\n\n/* Responsive Design */\n@media (max-width: 1024px) {\n    .contact-content {\n        grid-template-columns: 1fr;\n        gap: 3rem;\n    }\n    \n    .contact-form-container {\n        padding: 2rem;\n    }\n}\n\n@media (max-width: 768px) {\n    .contact-section {\n        padding: 4rem 0;\n    }\n    \n    .contact-hero {\n        min-height: 40vh;\n    }\n    \n    .contact-form .form-row {\n        grid-template-columns: 1fr;\n        gap: 1rem;\n    }\n    \n    .contact-form-container {\n        padding: 1.5rem;\n    }\n    \n    .contact-method {\n        padding: 1rem;\n    }\n}\n\n@media (max-width: 480px) {\n    .contact-form-container {\n        padding: 1rem;\n    }\n}"}
