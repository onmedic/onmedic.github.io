[2025-08-22 17:34:12] Write: {"file_path": "/Users/albertarno/Documents/_codi/onmedic.github.io/index.html", "content": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>onmedic - Building eHealth | Digital Healthcare Solutions</title>\n    <meta name=\"description\" content=\"onmedic creates innovative healthcare technology. AI solutions for hospitals, pharmaceutical companies, and medical institutions worldwide.\">\n    \n    <!-- Hreflang for all language versions -->\n    <link rel=\"alternate\" hreflang=\"ca\" href=\"https://onmedic.com/ca/\">\n    <link rel=\"alternate\" hreflang=\"es\" href=\"https://onmedic.com/es/\">\n    <link rel=\"alternate\" hreflang=\"en\" href=\"https://onmedic.com/en/\">\n    <link rel=\"alternate\" hreflang=\"x-default\" href=\"https://onmedic.com/en/\">\n    \n    <!-- Canonical pointing to default English version -->\n    <link rel=\"canonical\" href=\"https://onmedic.com/en/\">\n    \n    <!-- Prevent indexing of this redirect page -->\n    <meta name=\"robots\" content=\"noindex, follow\">\n    \n    <!-- Preload critical resources for faster redirects -->\n    <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\n    \n    <!-- Favicon -->\n    <link rel=\"icon\" type=\"image/x-icon\" href=\"/favicon.ico\">\n    <link rel=\"icon\" type=\"image/png\" sizes=\"32x32\" href=\"/favicon-32x32.png\">\n    <link rel=\"apple-touch-icon\" sizes=\"180x180\" href=\"/apple-touch-icon.png\">\n    \n    <!-- Basic styling for loading screen -->\n    <style>\n        body {\n            margin: 0;\n            padding: 0;\n            font-family: 'Inter', -apple-system, BlinkMacSystemFont, sans-serif;\n            background: linear-gradient(135deg, #0A2342 0%, #00D4FF 100%);\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            min-height: 100vh;\n            color: white;\n        }\n        .loader {\n            text-align: center;\n            max-width: 600px;\n            padding: 2rem;\n        }\n        .logo {\n            font-size: 3rem;\n            font-weight: 700;\n            margin-bottom: 1rem;\n        }\n        .loading-text {\n            font-size: 1.2rem;\n            opacity: 0.9;\n            margin-bottom: 2rem;\n        }\n        .spinner {\n            border: 3px solid rgba(255, 255, 255, 0.3);\n            border-radius: 50%;\n            border-top: 3px solid white;\n            width: 40px;\n            height: 40px;\n            animation: spin 1s linear infinite;\n            margin: 0 auto 2rem;\n        }\n        @keyframes spin {\n            0% { transform: rotate(0deg); }\n            100% { transform: rotate(360deg); }\n        }\n        .manual-links {\n            margin-top: 2rem;\n            font-size: 0.9rem;\n        }\n        .manual-links a {\n            color: white;\n            text-decoration: underline;\n            margin: 0 1rem;\n        }\n        .manual-links a:hover {\n            opacity: 0.8;\n        }\n        @media (max-width: 768px) {\n            .logo {\n                font-size: 2rem;\n            }\n            .loading-text {\n                font-size: 1rem;\n            }\n            .manual-links a {\n                display: block;\n                margin: 0.5rem 0;\n            }\n        }\n    </style>\n</head>\n<body>\n    <div class=\"loader\">\n        <div class=\"logo\">onmedic</div>\n        <div class=\"loading-text\">Detecting your language preference...</div>\n        <div class=\"spinner\"></div>\n        \n        <!-- Fallback manual links -->\n        <div class=\"manual-links\">\n            <p>Choose your language:</p>\n            <a href=\"/ca/\" onclick=\"setLanguagePreference('ca')\">Catal\u00e0</a>\n            <a href=\"/es/\" onclick=\"setLanguagePreference('es')\">Espa\u00f1ol</a>\n            <a href=\"/en/\" onclick=\"setLanguagePreference('en')\">English</a>\n        </div>\n    </div>\n\n    <script>\n        // Language detection and redirect logic\n        function detectAndRedirect() {\n            try {\n                // Check for previously stored language preference\n                const storedLang = localStorage.getItem('onmedic-language');\n                if (storedLang && ['ca', 'es', 'en'].includes(storedLang)) {\n                    redirectToLanguage(storedLang);\n                    return;\n                }\n\n                // Get browser languages (ordered by preference)\n                const browserLanguages = navigator.languages || [navigator.language || navigator.userLanguage || 'en'];\n                \n                // Language mapping with priority\n                const languageMap = {\n                    'ca': ['ca', 'ca-ES', 'ca-AD'],\n                    'es': ['es', 'es-ES', 'es-AR', 'es-MX', 'es-CO', 'es-PE', 'es-VE', 'es-CL', 'es-EC', 'es-GT', 'es-CU', 'es-BO', 'es-DO', 'es-HN', 'es-PY', 'es-SV', 'es-NI', 'es-CR', 'es-PA', 'es-UY', 'es-PR'],\n                    'en': ['en', 'en-US', 'en-GB', 'en-CA', 'en-AU', 'en-NZ', 'en-IE', 'en-ZA', 'en-IN']\n                };\n\n                // Find the best language match\n                let detectedLang = 'en'; // Default fallback\n                \n                for (const browserLang of browserLanguages) {\n                    const normalizedLang = browserLang.toLowerCase();\n                    \n                    // Check for exact matches first\n                    for (const [targetLang, variants] of Object.entries(languageMap)) {\n                        if (variants.includes(normalizedLang)) {\n                            detectedLang = targetLang;\n                            break;\n                        }\n                    }\n                    \n                    // If found, break the outer loop\n                    if (detectedLang !== 'en' || browserLang.toLowerCase().startsWith('en')) {\n                        break;\n                    }\n                    \n                    // Check for language code prefix (e.g., 'ca' from 'ca-FR')\n                    const langPrefix = normalizedLang.split('-')[0];\n                    if (langPrefix === 'ca') {\n                        detectedLang = 'ca';\n                        break;\n                    } else if (langPrefix === 'es') {\n                        detectedLang = 'es';\n                        break;\n                    }\n                }\n\n                // Special handling for geographic preferences\n                if (navigator.geolocation && detectedLang === 'en') {\n                    // Note: In a real implementation, you might want to check geographic location\n                    // for users who have English as their language but might prefer Spanish or Catalan\n                    // based on their location (Spain, Catalonia, Latin America)\n                }\n\n                // Store the detected language for future visits\n                localStorage.setItem('onmedic-language', detectedLang);\n                \n                // Redirect after a short delay to show the loading animation\n                setTimeout(() => redirectToLanguage(detectedLang), 800);\n                \n            } catch (error) {\n                console.error('Language detection error:', error);\n                // Fallback to English on any error\n                setTimeout(() => redirectToLanguage('en'), 500);\n            }\n        }\n\n        function redirectToLanguage(lang) {\n            const validLanguages = ['ca', 'es', 'en'];\n            const targetLang = validLanguages.includes(lang) ? lang : 'en';\n            \n            // Use replace to avoid back button issues\n            window.location.replace(`/${targetLang}/`);\n        }\n\n        function setLanguagePreference(lang) {\n            // Store user's manual language selection\n            localStorage.setItem('onmedic-language', lang);\n            redirectToLanguage(lang);\n        }\n\n        // Handle URL fragments/hashes and query parameters\n        function handleUrlParameters() {\n            const urlParams = new URLSearchParams(window.location.search);\n            const hash = window.location.hash;\n            \n            // Check for explicit language parameter\n            const langParam = urlParams.get('lang');\n            if (langParam && ['ca', 'es', 'en'].includes(langParam)) {\n                localStorage.setItem('onmedic-language', langParam);\n                let redirectUrl = `/${langParam}/`;\n                \n                // Preserve hash if present\n                if (hash) {\n                    redirectUrl += hash;\n                }\n                \n                window.location.replace(redirectUrl);\n                return true;\n            }\n            \n            return false;\n        }\n\n        // Initialize on page load\n        document.addEventListener('DOMContentLoaded', function() {\n            // Check for URL parameters first\n            if (!handleUrlParameters()) {\n                // Otherwise, proceed with normal language detection\n                detectAndRedirect();\n            }\n        });\n\n        // Handle case where DOMContentLoaded has already fired\n        if (document.readyState === 'loading') {\n            // Document is still loading\n            document.addEventListener('DOMContentLoaded', function() {\n                if (!handleUrlParameters()) {\n                    detectAndRedirect();\n                }\n            });\n        } else {\n            // Document has already loaded\n            if (!handleUrlParameters()) {\n                detectAndRedirect();\n            }\n        }\n    </script>\n\n    <!-- Structured Data for the organization -->\n    <script type=\"application/ld+json\">\n    {\n        \"@context\": \"https://schema.org\",\n        \"@type\": \"Organization\",\n        \"@id\": \"https://onmedic.com/#organization\",\n        \"name\": \"onmedic\",\n        \"alternateName\": \"onmedic Building eHealth\",\n        \"description\": \"Building eHealth through innovation and digital transformation in healthcare. We develop AI-powered solutions for pharmaceutical companies, hospitals, and healthcare institutions.\",\n        \"url\": \"https://onmedic.com\",\n        \"logo\": \"https://onmedic.com/src/assets/images/logo-onmedic.webp\",\n        \"sameAs\": [\n            \"https://linkedin.com/company/onmedic\"\n        ],\n        \"contactPoint\": {\n            \"@type\": \"ContactPoint\",\n            \"contactType\": \"customer service\",\n            \"email\": \"info@onmedic.com\"\n        },\n        \"address\": {\n            \"@type\": \"PostalAddress\",\n            \"addressCountry\": \"ES\",\n            \"addressRegion\": \"Catalunya\"\n        },\n        \"foundingDate\": \"2020\",\n        \"industry\": \"Healthcare Technology\",\n        \"keywords\": \"healthcare technology, AI solutions, digital health, pharmaceutical technology, hospital innovation, eHealth, medical software, healthcare digitization\",\n        \"knowsAbout\": [\n            \"Artificial Intelligence in Healthcare\",\n            \"Digital Transformation\",\n            \"Pharmaceutical Technology\",\n            \"Hospital Management Systems\",\n            \"Medical Data Analytics\",\n            \"Healthcare Software Development\",\n            \"eHealth Solutions\"\n        ]\n    }\n    </script>\n</body>\n</html>"}
